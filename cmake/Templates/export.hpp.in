#ifndef CAFFE_EXPORT_HPP_
#define CAFFE_EXPORT_HPP_

// CAFFE_BUILDING_STATIC_LIB should be defined
// only by the caffe target
#if defined(_MSC_VER) && !defined(CAFFE_BUILDING_STATIC_LIB)
    ${CAFFE_INCLUDE_SYMBOLS}
#endif

#if defined(_MSC_VER)
  #if !defined(CAFFE_EXPORT)
    #if defined(CAFFE_STATIC_LIB)
      #define CAFFE_EXPORT
    #elif defined(caffe_EXPORTS)
      #define CAFFE_EXPORT __declspec(dllexport)
    #else
      #define CAFFE_EXPORT __declspec(dllimport)
    #endif
  #endif
#else
  #define CAFFE_EXPORT
#endif

#endif  // CAFFE_EXPORT_HPP_